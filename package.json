{
    "name": "curative-intelligence-agent",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "node ./scripts/run-prisma-generate.js && next build",
        "start": "next start",
        "lint": "next lint",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "worker:start": "node ./src/workers/generation-worker.js",
        "db:generate": "node ./scripts/run-prisma-generate.js",
        "db:push": "prisma db push",
        "migrate:apply": "prisma migrate dev",
        "migrate:deploy": "powershell -ExecutionPolicy Bypass -File ./scripts/prisma-migrate-deploy.ps1",
        "migrate:sql": "node ./scripts/apply-sql-migrations.js",
        "db:backfill:clerkid": "node scripts/backfill-clerk-id.js",
        "db:clerkid:notnull": "prisma db execute --file ./scripts/set-clerkid-notnull.sql --schema ./prisma/schema.prisma"
    },
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@hookform/resolvers": "^5.1.1",
        "@prisma/client": "^6.11.1",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-slot": "^1.2.3",
        "@supabase/ssr": "^0.5.0",
        "@supabase/supabase-js": "^2.45.1",
        "bullmq": "^3.16.2",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "critters": "^0.0.23",
        "dotenv": "^17.2.3",
        "ioredis": "^5.8.0",
        "lucide-react": "^0.525.0",
        "next": "^16.1.0-canary.2",
        "pg": "^8.11.0",
        "prisma": "^6.10.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-hook-form": "^7.60.0",
        "resend": "^6.1.3",
        "tailwind-merge": "^3.3.1",
        "zod": "^3.25.69"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@testing-library/jest-dom": "^6.9.1",
        "@types/dotenv": "^6.1.1",
        "@types/node": "^20",
        "@types/pg": "^8.15.5",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@vitest/ui": "^3.2.4",
        "eslint": "^9",
        "eslint-config-next": "15.3.4",
        "jsdom": "^27.0.0",
        "playwright": "^1.56.1",
        "tailwindcss": "^4",
        "typescript": "^5",
        "vitest": "^3.2.4"
    }
}
